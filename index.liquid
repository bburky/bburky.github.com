    {%- for post in collections.published_entries reversed -%}
      <article
      id="{{post.data.slug}}"
      class="
        {%- if post.data.class -%}
          {{ post.data.class | join: " " }}
        {%- else -%}
          notecard-6x4
        {%- endif -%}
        {{ " " }}
        {{- post.data.tags | join: " " -}}
        {%- if post.data.url %} external{% endif -%}
      ">
        <header>
          {%- if post.data.cve -%}
          <ul class="cve-header">
            {%- for cve in post.data.cve -%}
              <li><a href="https://nvd.nist.gov/vuln/detail/{{ cve }}">{{ cve }}</a></li>
            {% endfor %}
          </ul>
          {%- endif -%}

          {%- if post.data.hideHeader != true -%}
          <h1>
            {%- if post.data.url -%}
            <a href="{{ post.data.url }}">
            {%- endif -%}
              {{- post.data.title -}}
            {%- if post.data.url -%}
            </a>
            {%- endif -%}
          </h1>
          {%- endif -%}
        </header>
          <section>
            {{- post | excerpt -}}
          </section>
      </article>
    {%- endfor -%}
  </body>
</html>